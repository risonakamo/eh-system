{
    "compilerOptions":{
        "strict":true,
        "jsx":"react",
        "lib":["dom","ESNext"],
        "target":"ESNext",
        "esModuleInterop":true,
        "baseUrl":".",
        "moduleResolution":"node",
        "paths": {
            "components/*":["web/components/*"],
            "lib/*":["web/lib/*"]
        }
    },
    "include":[
        "**/*.ts",
        "**/*.d.ts",
        "**/*.tsx"
    ],
    "exclude":[
        "node_modules"
    ]
}